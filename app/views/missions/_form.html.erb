<%= simple_form_for(@mission) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :class_name %>
    <%= f.input :class_code %>
    <%= f.input :variables %>
    <%= f.association :interface %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
