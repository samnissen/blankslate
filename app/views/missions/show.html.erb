<%- model_class = Mission -%>
<div class="page-header">
  <h1><%=t '.title', :default => Mission %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:class_name) %></dt>
      <dd><pre class="prettyprint"><%= @mission.class_name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:class_code) %></dt>
      <dd><pre class="prettyprint"><%= @mission.class_code %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:variables) %></dt>
      <dd><pre class="prettyprint"><%= @mission.variables %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              missions_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_mission_path(@mission), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              mission_path(@mission),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

